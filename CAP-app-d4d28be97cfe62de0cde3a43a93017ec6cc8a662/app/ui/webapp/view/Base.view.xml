<mvc:View
    controllerName="ui.controller.Base"
    xmlns:core="sap.ui.core" 
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m" xmlns:f="sap.f">
	<Page showHeader="false">
		<content>
			<f:FlexibleColumnLayout id="fcl" backgroundDesign="Solid">
				<f:beginColumnPages>
					<mvc:XMLView id="beginView" viewName="ui.view.Master"/>
				</f:beginColumnPages>
				<f:midColumnPages>
					<mvc:XMLView id="detailView" viewName="ui.view.Detail"/>
				</f:midColumnPages>
			</f:FlexibleColumnLayout>
		</content>
	</Page>
</mvc:View>