<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="valueHelpDialog"
		title="Címkék"
		items="{/Tags}"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose"
		multiSelect="true">
		<StandardListItem
			title="{name}">
            <customData>
                <core:CustomData key="{ID}"/>
            </customData>
        </StandardListItem>
	</SelectDialog>
</core:FragmentDefinition>